{
  "name": "react-webpack",
  "version": "1.0.0",
  "description": "react",
  "main": "index.js",
  "scripts": {
    "clear": "rimraf ./dist/build",
    "build:server": "cross-env NODE_ENV=development webpack --config build/webpack.server.config.babel.js --env=dev",
    "server": "cross-env NODE_ENV=development babel-node ./service/server.babel.js",
    "prebuild:dev": "npm run clear",
    "build:dev": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.dev.config.babel.js --env=dev",
    "build:prod": "cross-env NODE_ENV=production webpack --config build/webpack.prod.config.babel.js",
    "prebuild": "npm run clear && npm run build:server",
    "postbuild": "npm run server:babel",
    "server:dev": "cross-env NODE_ENV=development babel-node ./service/server.babel.js",
    "build": "npm run build:prod",
    "nodemon": "start npm run build:dev && nodemon ./service/server.babel.js"
  },
  "repository": {
    "type": "git",
    "url": "https://gitee.com/penyuying/react-webpack"
  },
  "keywords": [
    "react",
    "webpack"
  ],
  "author": "penyuying",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "core-js": "^2.5.4",
    "cross-env": "^5.1.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.3",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "express": "^4.16.3",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^3.2.0",
    "memory-fs": "^0.4.1",
    "minimist": "^1.2.0",
    "nodemon": "^1.17.3",
    "react-hot-loader": "^4.0.1",
    "transform-runtime": "0.0.0",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.1",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1"
  }
}
